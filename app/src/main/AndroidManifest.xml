<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.infinitybike">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".CriarLogin_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PerfilUsuario_Activity"></activity>
        <activity android:name=".RecebeCardView_Activity"></activity>
        <activity android:name=".OrdemPedidos_Activity" />
        <activity android:name=".Status_Activity" />
        <activity android:name=".MenuNavigation_Activity"></activity>
        <activity android:name=".Agendamento_Activity"></activity>
        <activity android:name=".DadosAgendamento_Activity"></activity>
        <activity android:name=".FormCadastro_Activity" />
        <activity android:name=".Login_Activity"></activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>